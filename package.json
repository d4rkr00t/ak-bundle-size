{
  "name": "ak-bundle-size",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@size-limit/file": "^5.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "start": "craco start",
    "build": "craco build",
    "clean": "rm -rf node_modules && rm yarn.lock && yarn",
    "size": "size-limit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "build/static/js/*.js"
    }
  ],
  "devDependencies": {
    "@atlaskit/editor-core": "latest",
    "@atlaskit/media-card": "latest",
    "@atlaskit/media-core": "latest",
    "@atlaskit/media-ui": "latest",
    "@atlaskit/media-viewer": "latest",
    "@atlaskit/media-image": "latest",
    "@atlaskit/media-filmstrip": "latest",
    "@atlaskit/media-picker": "latest",
    "@size-limit/preset-big-lib": "^5.0.5",
    "react-intl": "2.6.0",
    "rxjs": "^5.5.0",
    "size-limit": "^5.0.5",
    "styled-components": "^3.2.6"
  },
  "resolutions": {
    "prosemirror-collab": "^1.2.2",
    "prosemirror-commands": "^1.1.4",
    "prosemirror-history": "^1.1.3",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "1.1.4",
    "prosemirror-model": "1.11.0",
    "prosemirror-schema-list": "^1.1.4",
    "prosemirror-state": "1.3.3",
    "prosemirror-transform": "1.2.8",
    "prosemirror-utils": "^1.0.0-0",
    "prosemirror-view": "1.15.4"
  }
}
